{
  "name": "tree-sitter-gdscript",
  "version": "0.2.3",
  "description": "GDScript grammar for tree-sitter",
  "main": "index.js",
  "keywords": [
    "parser",
    "lexer"
  ],
  "author": "Preston Knopp",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.4.0",
    "prebuild-install": "^5.0.0"
  },
  "devDependencies": {
    "prebuild": "^7.6.0",
    "tree-sitter-cli": "^0.13.2"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuild -r electron -t 1.6.0 -t 1.7.0 -t 1.8.0 -t 2.0.0 -t 3.0.0 -t 4.0.0 --strip --verbose",
    "prebuild:upload": "prebuild --upload-all",
    "build": "tree-sitter generate && tree-sitter build",
    "buildTest": "tree-sitter generate && tree-sitter build && tree-sitter test",
    "test": "tree-sitter test",
    "test-windows": "node_modules/.bin/tree-sitter test"
  },
  "repository": "https://github.com/PrestonKnopp/tree-sitter-gdscript"
}
